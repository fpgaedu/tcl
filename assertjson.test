lappend auto_path .

package require Tcl 8.5
package require tcltest 2.3.8
package require fpgaedu::assert 1.0
package require fpgaedu::assert::json 1.0

namespace import ::tcltest::*
namespace import ::fpgaedu::assert::*
namespace import ::fpgaedu::assert::json::*

test "testAssertObjectContainsKey1" "" \
        -body {
            set json {{"test": "value"}}
            assertObjectContainsKey $json test
            assertThrows {
                assertObjectContainsKey $json invalidkey
            }
        }

test "testAssertObjectContainsKey2" "" \
        -body {
            set json {{"outer": {"inner": "value"}}}
            assertObjectContainsKey $json outer inner
            assertThrows {
                assertObjectContainsKey $json outer invalidkey
            }
        }

test "testAssertObjectContainsKey3" "" \
        -body {
            set json {["outer", "inner"]}
            assertThrows {
                assertObjectContainsKey $json outer
            }
        }

cleanupTests